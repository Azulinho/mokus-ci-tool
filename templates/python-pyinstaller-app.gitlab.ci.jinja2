# vim:filetype=yaml

# {{ git_url }}

before_script:
  - apt-get update
  - apt-get install -y make python-dev python-pip python-virtualenv

make_all:
  stage: clean
  script:
    - make clean
    - make setup
    - make build
    - make test
    - make package
    - make publish
  tags:
    - shell
    - build-as-docker
  cache:
    key: "$CI_BUILD_REF"
    paths:
      - artifacts/
      - venv/
      - build/
      - dist/
  artifacts:
    paths:
      - artifacts/mokus-ci-tool


